import QmlProject 1.1

Project {
    mainFile: "main.qml"

    /* Include .qml, .js, and image files from current directory and subdirectories */
    QmlFiles {
        directory: "."
    }

    ImageFiles {
        directory: "./img"
    }

    Files {
        filter: "*.ttf;*.otf"
        directory: "./fonts"
    }
}
